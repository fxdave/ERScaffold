

    <% entity.getBelongsToRelations().forEach( (relation) => { %>
    public function <%= relation.getTo().name.toLowerCase() %>()
    {
        return $this->belongsTo(App\<%= relation.getTo().name %>::class);
    }
    <% }); %>

    <% entity.getHasManyRelations().forEach( (relation) => { %>
    public function <%= relation.getTo().name.toLowerCase() %>()
    {
        return $this->hasMany(App\<%= relation.getTo().name %>::class);
    }
    <% }); %>

    <% entity.getHasOneRelations().forEach( (relation) => { %>
    public function <%= relation.getTo().name.toLowerCase() %>()
    {
        return $this->hasOne(App\<%= relation.getTo().name %>::class);
    }
    <% }); %>

    <% entity.getBelongsToManyRelations().forEach( (relation) => { %>
    public function <%= relation.getTo().name.toLowerCase() %>()
    {
        return $this->belongsToMany(App\<%= relation.getTo().name %>::class);
    }
    <% }); %>